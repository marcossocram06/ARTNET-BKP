add bridge=bridge1 interface=ether5
add bridge=bridge1 interface=sfp1
/interface pppoe-server server
# Service is on a slave interface
add disabled=no interface=sfp1 service-name=service1
/ip address
add address=192.168.40.1/24 interface=bridge1 network=192.168.40.0
add address=192.168.147.2/24 interface=ether1 network=192.168.147.0
/ip route
