/interface bridge
add name=bridge1
/interface ethernet
set [ find default-name=ether1 ] comment="Entrada mimosa"
set [ find default-name=sfp1 ] comment="Sida LinkOLT"
/interface vlan
add interface=ether1 name=vlan2049 vlan-id=2049
/interface wireless security-profiles
set [ find default=yes ] supplicant-identity=MikroTik
