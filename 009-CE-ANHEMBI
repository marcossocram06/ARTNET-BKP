    \_comment~\"IXCSoft PPPOE\" ] do={ enable  \$i };" host=207.248.10.142 \
    interval=5m up-script="/ppp secret ; :foreach i in [ find comment~\"IXCSoft \
    PPPOE\" ] do={ disable \$i }; /ppp active; :foreach p in [find comment~\"IXC\
    Soft\"] do={ remove \$p; :delay 1};"
