    ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon \
    start-date=jan/10/2024 start-time=00:00:00
/system script
add dont-require-permissions=no name=bkp-procyon owner=bruno-procyon policy=\
    ftp,reboot,read,write,policy,test,password,sniff,sensitive,romon source=":lo\
    cal username \"bkpftpsocial\"\r\
    \n:local password \"!@#Social2401\"\r\
    \n:local ftpserver \"191.252.193.112\"\r\
    \n\r\
    \n# months array\r\
    \n:local months (\"jan\",\"feb\",\"mar\",\"apr\",\"may\",\"jun\",\"jul\",\"a\
    ug\",\"sep\",\"oct\",\"nov\",\"dec\");\r\
    \n\r\
    \n# get time\r\
    \n:local ts [/system clock get time]\r\
    \n:set ts ([:pick \$ts 0 2].[:pick \$ts 3 5].[:pick \$ts 6 8])\r\
    \n\r\
    \n# get Date\r\
    \n:local ds [/system clock get date]\r\
    \n# convert name of month to number\r\
    \n:local month [ :pick \$ds 0 3 ];\r\
    \n:local mm ([ :find \$months \$month -1 ] + 1);\r\
    \n:if (\$mm < 10) do={ :set mm (\"0\" . \$mm); }\r\
    \n# set \$ds to format YYYY-MM-DD\r\
    \n:set ds ([:pick \$ds 7 11] . \$mm . [:pick \$ds 4 6])\r\
    \n\r\
    \n# file name for config export - file name will be UMDB-servername-date-tim\
    e.rsc\r\
    \n:local filename ([/system identity get name].\"-\".\$ds.\"-\".\$ts.\".rsc\
    \")\r\
    \n\r\
    \n# backup the data\r\
    \n#/tool user-manager database save name=\$filename\r\
    \n/export compact file=\$filename\r\
    \n:log warning message=\"Config export finished (2/2).\"\r\
    \n\r\
    \n# Upload backup file to FTP server.\r\
    \n\r\
    \n/tool fetch address=\$ftpserver src-path=\"\$filename\" \\\r\
    \nuser=\$username mode=ftp password=\$password \\\r\
    \ndst-path=\"/Mikrotiks/SOCIALNET/SP-PQB-A01-BDR-RT-001/\$filename\" upload=\
    yes\r\
    \n\r\
    \n:log info \"Backup Uploaded Successfully\"\r\
    \n\r\
    \n# Delete created backup files once they have been uploaded\r\
    \n# so they don't accumulate and fill up storage space on the router.\r\
    \n\r\
    \n/file remove \"\$filename\"\r\
    \n\r\
    \n:log info \"Local Backup Files Deleted Successfully\"\r\
    \n"
/tool romon
set enabled=yes secrets=smf0705
